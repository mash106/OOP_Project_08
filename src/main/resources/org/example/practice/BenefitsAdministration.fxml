<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="668.0" prefWidth="629.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BenefitsAdministrationController">
    <ComboBox fx:id="employeeComboBox" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0" />

    <TableView fx:id="benefitsTableView" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="80.0">
        <columns>
            <TableColumn fx:id="benefitTypeColumn" text="Benefit Type" />
            <TableColumn fx:id="coverageColumn" text="Coverage" />
            <TableColumn fx:id="startDateColumn" text="Start Date" />
            <TableColumn fx:id="endDateColumn" text="End Date" />
        </columns>
    </TableView>

    <Button fx:id="addBenefitsButton" text="Add Benefits" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="20.0" />

    <Button fx:id="updateBenefitsButton" text="Update Benefits" AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="20.0" />
</AnchorPane>
